{
	"name": "node-red-contrib-selenium-wd2",
	"version": "0.1.6",
	"description": "Selenium-webdriver nodes for Node-RED based on node-red-contrib-selenium-webdriver",
	"dependencies": {
		"rxjs": "~6.6.3",
		"selenium-webdriver": "4.0.0-alpha.7"
	},
	"devDependencies": {
		"@types/node-red": "~1.1.0",
		"@types/node-red-node-test-helper": "~0.2.1",
		"@types/selenium-webdriver": "~3.0.8",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"eslint": "^8.54.0",
		"eslint-plugin-jsdoc": "^46.9.0",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"prettier": "~2.1.2",
		"tslint-config-prettier": "~1.18.0",
		"typescript": "~5.2.2"
	},
	"license": "Apache-2.0",
	"keywords": [
		"node-red",
		"selenium-wd2"
	],
	"node-red": {
		"nodes": {
			"selenium-wd2": "dist/selenium-wd2.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/simonradier/node-red-contrib-selenium-wd2"
	},
	"engines": {
		"node": ">= 14.0.0"
	},
	"scripts": {
		"lint": "eslint ./src/ && prettier --check .",
		"format": "prettier --write .",
		"prepublishOnly": "mkdir dist && cp -r src/icons dist/icons && cat src/nodes/*.html > dist/selenium-wd2.html && tsc",
		"clean": "rm -rf dist"
	},
	"author": {
		"name": "Simon Radier"
	}
}
